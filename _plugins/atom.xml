---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Jose Diaz-Gonzalez</title>
 <link href="http://josediazgonzalez.com/atom.xml" rel="self"/>
 <link href="http://josedizgonzalez.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://josediazgonzalez.com/</id>
 <author>
   <name>Jose Diaz-Gonzalez</name>
   <email>support@savant.be</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://josediazgonzalez.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://josediazgonzalez.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape | truncatewords: 20,'' }}</content>
 </entry>
 {% endfor %}
 
</feed>